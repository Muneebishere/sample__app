source 'https://rubygems.org'
gem 'rails', '~> 5.0.0'
gem 'bootstrap-sass'
group :development, :test do
  gem 'sqlite3'
  gem 'rspec-rails'
  gem 'selenium-webdriver'
end
# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end
gem 'capybara'
gem 'jquery-rails'
group :production do
  gem 'pg'
end
